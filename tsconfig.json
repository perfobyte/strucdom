{
    "compilerOptions": {
      "target": "es2020",
      "module": "esnext",
      "allowJs": true,
      "checkJs": false,
      "noEmitOnError": false,
      "removeComments": true,
      "outDir": "dist",
      "declaration": false,
      "strict": false,
      "noImplicitAny": false,
      "skipLibCheck": true
    },
    "include": ["src/**/*"]
}